{
 "name": "@sellmate/design-system-core",
 "version": "0.0.1",
 "description": "Sellmate Design System - Core Web Components",
 "type": "module",
 "private": true,
 "main": "dist/index.cjs.js",
 "module": "dist/index.js",
 "types": "dist/types/index.d.ts",
 "collection": "dist/collection/collection-manifest.json",
 "collection:main": "dist/collection/index.js",
 "unpkg": "dist/design-system/design-system.esm.js",
 "files": [
  "dist/",
  "loader/",
  "www/"
 ],
 "exports": {
  ".": {
   "import": "./dist/design-system/design-system.esm.js",
   "require": "./dist/design-system/design-system.cjs.js"
  },
  "./components/*": {
   "types": "./dist/components/*.d.ts",
   "import": "./dist/components/*.js"
  },
  "./loader": {
   "types": "./loader/index.d.ts",
   "import": "./loader/index.js",
   "require": "./loader/index.cjs"
  }
 },
 "workspaces": [
  "packages/*"
 ],
 "repository": {
  "type": "git",
  "url": "https://gitlab.corp.sellmate.co.kr/sellmate/frontend/design-system.git"
 },
 "scripts": {
  "build": "stencil build",
  "build:watch": "stencil build --dev --watch",
  "build:all": "yarn build && yarn build:packages",
  "build:packages": "cd packages/react && yarn build && cd ../vue && yarn build",
  "build:tokens": "npx tsx src/styles/build-tokens.ts",
  "start": "stencil build --dev --watch --serve",
  "test": "stencil test --spec --e2e",
  "test.watch": "stencil test --spec --e2e --watchAll",
  "generate": "stencil generate",
  "lint": "eslint 'src/**/*.{ts,tsx}' --fix",
  "format": "prettier --write 'src/**/*.{ts,tsx,scss,css,html,json}'",
  "storybook": "storybook dev -p 6006",
  "build-storybook": "storybook build",
  "publish:all": "yarn build:all && yarn workspaces foreach --no-private npm publish"
 },
 "devDependencies": {
  "@chromatic-com/storybook": "^4.1.1",
  "@stencil/core": "^4.27.1",
  "@stencil/eslint-plugin": "^1.1.0",
  "@stencil/react-output-target": "^1.2.0",
  "@stencil/sass": "^3.2.2",
  "@stencil/vue-output-target": "^0.11.8",
  "@storybook/addon-a11y": "^9.1.10",
  "@storybook/addon-docs": "^9.1.10",
  "@storybook/addon-vitest": "^9.1.10",
  "@storybook/web-components-vite": "^9.1.10",
  "@types/jest": "^29.5.14",
  "@types/node": "^22.13.5",
  "@types/react": "^19.2.2",
  "@types/react-dom": "^19.2.2",
  "@vitest/browser": "^3.2.4",
  "@vitest/coverage-v8": "^3.2.4",
  "eslint": "^9.37.0",
  "eslint-config-prettier": "^10.1.8",
  "eslint-plugin-import": "^2.32.0",
  "eslint-plugin-jsx-a11y": "^6.10.2",
  "eslint-plugin-prettier": "^5.5.4",
  "eslint-plugin-simple-import-sort": "^12.1.1",
  "eslint-plugin-storybook": "^9.1.10",
  "jest": "^29.7.0",
  "jest-cli": "^29.7.0",
  "lit": "^3.3.1",
  "playwright": "^1.56.0",
  "prettier": "^3.6.2",
  "puppeteer": "^24.3.0",
  "storybook": "^9.1.10",
  "typescript": "^5.9.3",
  "vitest": "^3.2.4"
 },
 "license": "MIT",
 "dependencies": {
  "sass": "^1.93.2"
 }
}
